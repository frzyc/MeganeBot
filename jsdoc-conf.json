{
    "opts": {
        "destination": "./docs/",   
        "recurse": true
    },
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include": [ "./MeganeClient" ],
        "exclude": [ "./MeganeClient/node_modules" ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false
    }
}